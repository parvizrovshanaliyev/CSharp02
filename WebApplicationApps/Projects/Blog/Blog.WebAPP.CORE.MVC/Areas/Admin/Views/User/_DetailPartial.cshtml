@model Blog.Entities.Dtos.User.UserDto

<div class="form-row">
    <div class="form-group col-md-6">
        <label asp-for="UserName"></label>
        <input asp-for="UserName" class="form-control" autocomplete="off" disabled="disabled"/>
        <span asp-validation-for="UserName" class="text-danger"></span>
    </div>
    <div class="form-group col-md-6">
        <label asp-for="Email"></label>
        <input asp-for="Email" type="email" class="form-control" autocomplete="off" disabled="disabled"/>
        <span asp-validation-for="Email" class="text-danger"></span>
    </div>
    <div class="form-group col-md-6">
        <label asp-for="PhoneNumber"></label>
        <input asp-for="PhoneNumber" type="text" class="form-control" autocomplete="off" disabled="disabled"/>
        <span asp-validation-for="PhoneNumber" class="text-danger"></span>
    </div>

    <div class="form-group col-md-6">
        <label asp-for="FirstName"></label>
        <input asp-for="FirstName" type="text" class="form-control" autocomplete="off" disabled="disabled"/>
        <span asp-validation-for="FirstName" class="text-danger"></span>
    </div>
    <div class="form-group col-md-6">
        <label asp-for="LastName"></label>
        <input asp-for="LastName" type="text" class="form-control" autocomplete="off" disabled="disabled"/>
        <span asp-validation-for="LastName" class="text-danger"></span>
    </div>

    <div class="form-group col-md-12">
        <label asp-for="AboutMe"></label>
        <textarea class="form-control" asp-for="AboutMe"  rows="4" disabled="disabled"></textarea>
        <span class="text-danger" asp-validation-for="AboutMe"></span>
    </div>

    <div class="form-group col-md-6">
        <label asp-for="TwitterLink"></label>
        <input type="text" class="form-control" asp-for="TwitterLink" disabled="disabled"/>
        <span class="text-danger" asp-validation-for="TwitterLink"></span>
    </div>
    <div class="form-group col-md-6">
        <label asp-for="FacebookLink"></label>
        <input type="text" class="form-control" asp-for="FacebookLink" disabled="disabled"/>
        <span class="text-danger" asp-validation-for="FacebookLink"></span>
    </div>
    <div class="form-group col-md-6">
        <label asp-for="InstagramLink"></label>
        <input type="text" class="form-control" asp-for="InstagramLink" disabled="disabled"/>
        <span class="text-danger" asp-validation-for="InstagramLink"></span>
    </div>
    <div class="form-group col-md-6">
        <label asp-for="LinkedInLink"></label>
        <input type="text" class="form-control" asp-for="LinkedInLink" disabled="disabled"/>
        <span class="text-danger" asp-validation-for="LinkedInLink"></span>
    </div>
    <div class="form-group col-md-6">
        <label asp-for="YoutubeLink"></label>
        <input type="text" class="form-control" asp-for="YoutubeLink" disabled="disabled"/>
        <span class="text-danger" asp-validation-for="YoutubeLink"></span>
    </div>
    <div class="form-group col-md-6">
        <label asp-for="GitHubLink"></label>
        <input type="text" class="form-control" asp-for="GitHubLink" disabled="disabled"/>
        <span class="text-danger" asp-validation-for="GitHubLink"></span>
    </div>
    <div class="form-group col-md-6">
        <label asp-for="WebsiteLink"></label>
        <input type="text" class="form-control" asp-for="WebsiteLink" disabled="disabled"/>
        <span class="text-danger" asp-validation-for="WebsiteLink"></span>
    </div>
</div>
